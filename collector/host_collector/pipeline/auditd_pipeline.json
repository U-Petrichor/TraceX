{
  "description": "Map Auditd logs to TraceX Unified Event Schema",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": ["type=%{WORD:raw_type} msg=audit\\(%{NUMBER:audit_epoch}:%{NUMBER:event_id}\\): %{GREEDYDATA:kv_part}"]
      }
    },
    {
      "kv": {
        "field": "kv_part",
        "field_split": " ",
        "value_split": "="
      }
    },
    {
      "date": {
        "field": "audit_epoch",
        "formats": ["UNIX"],
        "target_field": "@timestamp"
      }
    },
    {
      "set": {
        "field": "event.dataset",
        "value": "auditd"
      }
    },
    {
      "set": {
        "field": "event.id",
        "value": "{{event_id}}"
      }
    },
    {
      "set": {
        "field": "event.category",
        "value": "process"
      }
    },
    {
      "set": {
        "field": "host.os.family",
        "value": "linux"
      }
    },
    {
      "rename": {
        "field": "kv.pid",
        "target_field": "process.pid",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "kv.comm",
        "target_field": "process.name",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "kv.exe",
        "target_field": "process.executable",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "kv.uid",
        "target_field": "process.user.id",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "kv.key",
        "target_field": "event.action",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "kv_part",
        "target_field": "raw.data",
        "ignore_missing": true
      }
    },
    {
      "rename": {
        "field": "raw_type",
        "target_field": "raw.type",
        "ignore_missing": true
      }
    },
    {
      "convert": {
        "field": "process.pid",
        "type": "integer",
        "ignore_missing": true
      }
    },
    {
      "remove": {
        "field": ["kv", "audit_epoch", "event_id"]
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}